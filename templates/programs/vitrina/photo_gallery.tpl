<div class="vitrina-photos">
	<div class="photo-gallery nano">	
		<div class="nanoScroller content">	
			{photos}
		</div>
	</div>
	<div class="photo-holder">
		<div class="photo-background"></div>
		<div class="photo-wrapper">
			<div class="photo">
				<img src="templates/programs/pepsimusics/assets/n1/IMG_3424.jpg">
			</div>
			<div class="photo-menu">
				<div class="like-photo"><img src="templates/programs/pepsimusics/assets/like.png" width="25" /></div>
				<div class="dislike-photo"><img src="templates/programs/pepsimusics/assets/dislike.png" width="25" /></div>
				<div class="close-photo"><button class="close">&times;</button></div>
			</div>
		</div>
	</div>
	<div class="zoom-holder">
		<div class="zoom-wrapper">
			<div class="zoom-background"></div>
			<div class="zoom"><i class="icon-white icon-search"></i></div>
		</div>
	</div>
</div>